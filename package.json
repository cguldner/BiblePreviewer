{
  "name": "bible-previewer",
  "version": "1.3.4",
  "description": "Turns every bible verse into a link you can hover over to see the contents of the verse",
  "main": "biblePreviewer.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cguldner/BiblePreviewer.git"
  },
  "scripts": {
    "build:prod": "webpack -p --env.NODE_ENV=production",
    "build:dev": "webpack",
    "clean": "rm -rf dist *.zip",
    "zip": "rm -rf *.zip && npm run build:dev && gulp zip",
    "watch": "webpack --watch",
    "cypress:gui": "cypress open",
    "cypress:run": "cypress run --record false --browser chrome",
    "test": "npm run build:dev && npm run cypress:run",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test"
    }
  },
  "author": "Cody Guldner",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cguldner/BiblePreviewer/issues"
  },
  "homepage": "https://github.com/cguldner/BiblePreviewer#readme",
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/preset-env": "^7.12.7",
    "autoprefixer": "^10.0.2",
    "babel-loader": "^8.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.2",
    "css-loader": "^5.0.1",
    "cypress": "^6.2.0",
    "cypress-intellij-reporter": "^0.0.5",
    "cypress-real-events": "^1.1.0",
    "eslint": "^7.14.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-regexp": "^0.1.1",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-zip": "^5.0.2",
    "husky": "^4.3.6",
    "mini-css-extract-plugin": "^1.3.1",
    "node-sass": "^5.0.0",
    "postcss": "^8.1.9",
    "postcss-loader": "^4.1.0",
    "sass-loader": "^10.1.0",
    "start-server-and-test": "^1.11.7",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.5.4",
    "materialize-css": "^1.0.0",
    "tippy.js": "^6.2.7"
  }
}
