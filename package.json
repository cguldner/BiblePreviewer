{
  "name": "bible-previewer",
  "version": "1.3.8",
  "description": "Turns every bible verse into a link you can hover over to see the contents of the verse",
  "main": "biblePreviewer.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cguldner/BiblePreviewer.git"
  },
  "scripts": {
    "build:prod": "webpack -p --env.NODE_ENV=production",
    "build:dev": "webpack",
    "clean": "rm -rf dist *.zip",
    "zip": "rm -rf *.zip && npm run build:dev && gulp zip",
    "watch": "webpack --watch",
    "cypress:gui": "cypress open",
    "cypress:run": "cypress run --record false --browser chrome --headed",
    "test": "npm run build:dev && npm run cypress:run",
    "lint": "eslint . --max-warnings 0 && stylelint **/*.scss",
    "lint:fix": "eslint . --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test"
    }
  },
  "author": "Cody Guldner",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cguldner/BiblePreviewer/issues"
  },
  "homepage": "https://github.com/cguldner/BiblePreviewer#readme",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.2",
    "css-loader": "^6.2.0",
    "cypress": "^9.2.0",
    "cypress-intellij-reporter": "0.0.7",
    "cypress-real-events": "^1.6.0",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.4.2",
    "eslint-plugin-no-only-tests": "^2.6.0",
    "eslint-plugin-regexp": "^1.5.1",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-zip": "^5.1.0",
    "husky": "^5.0.0",
    "mini-css-extract-plugin": "^1.3.1",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "sass-loader": "^12.4.0",
    "start-server-and-test": "^1.12.3",
    "stylelint": "^14.2.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "webpack": "^5.49.0",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.0",
    "materialize-css": "^1.0.0",
    "tippy.js": "^6.3.7"
  }
}
