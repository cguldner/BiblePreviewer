{
  "name": "bible-previewer",
  "version": "1.1.2",
  "description": "Turns every bible verse into a link you can hover over to see the contents of the verse",
  "main": "biblePreviewer.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cguldner/BiblePreviewer.git"
  },
  "scripts": {
    "build:prod": "node_modules/.bin/webpack -p --env.NODE_ENV=production",
    "build:dev": "node_modules/.bin/webpack",
    "clean": "rm -rf dist *.zip",
    "zip": "npm run build:prod && gulp zip",
    "watch": "node_modules/.bin/webpack --watch"
  },
  "author": "Cody Guldner",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cguldner/BiblePreviewer/issues"
  },
  "homepage": "https://github.com/cguldner/BiblePreviewer#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "autoprefixer": "^9.4.7",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "gulp": "^4.0.0",
    "gulp-zip": "^4.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "popper.js": "^1.14.7",
    "tooltip.js": "^1.3.1"
  }
}
